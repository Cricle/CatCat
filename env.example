# CatCat Environment Variables Example
# Copy this file to .env and update with your actual values

# Database Configuration
POSTGRES_DB=catcat
POSTGRES_USER=catcat
POSTGRES_PASSWORD=change_this_password_in_production

# Redis Configuration
REDIS_PASSWORD=change_this_redis_password

# JWT Configuration
JWT_SECRET_KEY=change-this-super-secret-key-must-be-at-least-32-characters-long
JWT_ISSUER=CatCat.API
JWT_AUDIENCE=CatCat.Client
JWT_EXPIRY_MINUTES=1440

# Stripe Payment Configuration
STRIPE_SECRET_KEY=sk_live_your_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_live_your_publishable_key_here

# OpenTelemetry Configuration
OTEL_SERVICE_NAME=CatCat.API
OTEL_EXPORTER_OTLP_ENDPOINT=http://jaeger:4317

# Application URLs
API_BASE_URL=http://localhost:5000
WEB_BASE_URL=http://localhost:3000

# Environment
ASPNETCORE_ENVIRONMENT=Production

